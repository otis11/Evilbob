{
	"manifest_version": 3,
	"name": "bob",
	"description": "bob",
	"version": "0.1",
	"icons": {
		"16": "images/icon-16.png",
		"32": "images/icon-32.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
	"background": {
		"service_worker": "background.js"
	},
	"options_page": "options-view/index.html",
	"action": {
		"default_icon": {
			"16": "images/icon-16.png",
			"32": "images/icon-32.png",
			"48": "images/icon-48.png",
			"128": "images/icon-128.png"
		}
	},
	"commands": {
		"bob.open": {
			"suggested_key": {
				"default": "Ctrl+Shift+L"
			},
			"description": "Open bob",
			"global": true
		}
	},
	"permissions": ["storage"],
	"optional_permissions": ["bookmarks", "system.cpu", "history"],
	"optional_host_permissions": ["https://*/*"]
}
